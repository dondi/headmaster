# This is a sample properties file.
#
# Copy this file to a file named build.properties in the root directory of
# the project, and set the actual property values accordingly.

# This example sets the web service to be on the same host as this web app,
# on port 8080.
service.root=http://localhost:8080/headmaster/

# Set this to true to have Wicket run in deployment mode.
wicket.deployment=false
